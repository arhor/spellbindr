<?xml version="1.0" encoding="utf-8"?>
<!--
   Privacy default for legacy Auto Backup behavior.
   android:allowBackup is disabled, but keep explicit exclusions as defense in depth.
-->
<full-backup-content>
    <exclude
        domain="database"
        path="." />
    <exclude
        domain="file"
        path="datastore/" />
</full-backup-content>
