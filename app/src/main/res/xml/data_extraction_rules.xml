<?xml version="1.0" encoding="utf-8"?>
<!--
   Privacy default: do not transfer application data by backup/device transfer.
   android:allowBackup is also disabled in AndroidManifest.xml.
-->
<data-extraction-rules>
    <cloud-backup>
        <exclude
            domain="database"
            path="." />
        <exclude
            domain="file"
            path="datastore/" />
    </cloud-backup>
    <device-transfer>
        <exclude
            domain="database"
            path="." />
        <exclude
            domain="file"
            path="datastore/" />
    </device-transfer>
</data-extraction-rules>
